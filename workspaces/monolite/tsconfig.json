{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": ["es5", "es2015.core", "es2015.proxy"],
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "declaration": true,
    "rootDir": "src",
    "outDir": "dist"
  }
}
